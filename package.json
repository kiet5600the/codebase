{
  "name": "HelloWorld",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "pod": "cd ios && pod install && cd ..",
    "android": "react-native run-android --variant=DevDebug",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "clean-ios": "cd ios && xcodebuild clean && cd ..",
    "clean-android-dev": "ENVFILE=env/.dev cd android && ./gradlew clean '-PdefaultEnvFile=env/.dev' && cd ..",
    "clean-android-staging": "ENVFILE=env/.staging cd android && ./gradlew clean '-PdefaultEnvFile=env/.staging' && cd ..",
    "clean-android-prod": "ENVFILE=env/.prod cd android && ./gradlew clean '-PdefaultEnvFile=env/.prod' && cd ..",
    "android:staging": "ENVFILE=env/.staging && react-native run-android --variant=stagingDebug",
    "android:preprod": "ENVFILE=env/.preprod && react-native run-android --variant=prodDebug",
    "apk:dev": "ENVFILE=env/.dev && cd android && ./gradlew clean '-PdefaultEnvFile=env/.dev' && ./gradlew assembleDevRelease '-PdefaultEnvFile=env/.dev' && cd ..",
    "apk:staging": "ENVFILE=env/.staging && cd android && ./gradlew clean '-PdefaultEnvFile=env/.staging' && ./gradlew assembleStagingRelease '-PdefaultEnvFile=env/.staging' && cd ..",
    "apk:prod": "ENVFILE=env/.prod && cd android && ./gradlew clean '-PdefaultEnvFile=env/.prod' && ./gradlew assembleProdRelease '-PdefaultEnvFile=env/.prod' && cd ..",
    "aab:dev": "ENVFILE=env/.dev && cd android && ./gradlew clean '-PdefaultEnvFile=env/.dev' && ./gradlew bundleRelease '-PdefaultEnvFile=env/.dev' && cd ..",
    "aab:staging": "ENVFILE=env/.staging && cd android && ./gradlew clean '-PdefaultEnvFile=env/.staging' && ./gradlew bundleRelease '-PdefaultEnvFile=env/.staging' && cd ..",
    "aab:prod": "ENVFILE=env/.prod && cd android && ./gradlew clean '-PdefaultEnvFile=env/.prod' && ./gradlew bundleRelease '-PdefaultEnvFile=env/.prod' && cd ..",
    "ios:staging": "ENVFILE=env/.staging react-native run-ios --scheme 'HelloWorld Staging'",
    "ios:prod": "ENVFILE=env/.prod react-native run-ios --scheme 'HelloWorld Release'",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "type-check": "tsc"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.14.0",
    "@gorhom/bottom-sheet": "^4.4.5",
    "@gorhom/portal": "^1.0.14",
    "@react-native-async-storage/async-storage": "^1.17.10",
    "@react-native-community/blur": "^4.2.0",
    "@react-native-community/netinfo": "^9.3.4",
    "@react-native-masked-view/masked-view": "^0.2.8",
    "@react-navigation/bottom-tabs": "^6.4.0",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/native-stack": "^6.9.1",
    "@reduxjs/toolkit": "^1.8.6",
    "@shopify/flash-list": "^1.3.1",
    "axios": "^1.1.2",
    "i18next": "^21.10.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "prop-types": "^15.8.1",
    "react": "18.1.0",
    "react-content-loader": "^6.2.0",
    "react-fast-compare": "^3.2.0",
    "react-hook-form": "^7.37.0",
    "react-i18next": "^11.18.6",
    "react-native": "0.70.3",
    "react-native-avoid-softinput": "^3.0.2",
    "react-native-bootsplash": "^4.3.2",
    "react-native-config": "^1.4.11",
    "react-native-country-picker-modal": "^2.0.0",
    "react-native-encrypted-storage": "^4.0.2",
    "react-native-fast-image": "^8.6.1",
    "react-native-flipper": "^0.163.0",
    "react-native-floating-label-input": "^1.4.2",
    "react-native-gesture-handler": "^2.7.1",
    "react-native-haptic-feedback": "^1.14.0",
    "react-native-indicators": "^0.17.0",
    "react-native-inkwell": "^0.4.5",
    "react-native-keyboard-manager": "^6.5.4-4",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-localize": "^2.2.4",
    "react-native-mmkv": "^2.5.0",
    "react-native-modal": "^13.0.1",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-reanimated-carousel": "^3.0.6",
    "react-native-rename": "^2.9.0",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.1",
    "react-native-sensors": "^7.3.5",
    "react-native-svg": "^13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-toast-message": "^2.1.5",
    "react-native-vector-icons": "^9.2.0",
    "react-redux": "^8.0.4",
    "redux-flipper": "^2.0.2",
    "redux-persist": "^6.0.0",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.187",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.4",
    "@types/react-native-indicators": "^0.16.2",
    "@types/react-native-vector-icons": "^6.4.12",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-import-resolver-babel-module": "^5.3.1",
    "eslint-plugin-ft-flow": "^2.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^27.1.1",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "patch-package": "^6.4.7",
    "prettier": "^2.7.1",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jest.setup.js"
    ],
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "/node_modules/(?!@react-native|react-native)"
    ]
  }
}
